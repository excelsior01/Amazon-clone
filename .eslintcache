Footers />\n            </Route>\n            <Route path='/payment'>\n              <Header />\n              <Elements stripe={promise}>\n                <Payment />\n              </Elements>\n              <Footers />\n            </Route>\n            <Route path='/'>\n              <Header />\n              <Shopping />\n              <Footers />\n            </Route>\n          </Switch>\n        </AnimatePresence>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n","C:\\Users\\murry\\Desktop\\REACT\\Ecommerce 1\\electronics\\src\\reportWebVitals.js",[],"C:\\Users\\murry\\Desktop\\REACT\\Ecommerce 1\\electronics\\src\\components\\Header.jsx",[],"C:\\Users\\murry\\Desktop\\REACT\\Ecommerce 1\\electronics\\src\\components\\Product.jsx",["93"],"import React, { useState } from \"react\";\r\nimport \"./styles/Product.scss\";\r\nimport { useStateValue } from \"./StateProvider\";\r\n\r\nfunction Product({ id, title, imgSource, price, rating, desc }) {\r\n  const [{ basket }, dispatch] = useStateValue();\r\n  const [isActive, setActive] = useState(false);\r\n\r\n  const addToBasket = () => {\r\n    // dispatch the item into the data layer\r\n    dispatch({\r\n      type: \"ADD_TO_BASKET\",\r\n      item: {\r\n        id: id,\r\n        title: title,\r\n        imgSource: imgSource,\r\n        price: price,\r\n        rating: rating,\r\n        desc: desc\r\n      },\r\n    });\r\n  };\r\n\r\n  const revealDetails = () => {\r\n    setActive(!isActive);\r\n  };\r\n\r\n  const hideDetails = () => {\r\n    setActive(!isActive);\r\n  };\r\n\r\n  return (\r\n    <div className='product'>\r\n      <div className='product__info'>\r\n        <p className='product__title'>{title}</p>\r\n        <p className='product__price'>\r\n          <small>$</small>\r\n          <strong>{price}</strong>\r\n        </p>\r\n        <div className='product__rating'>\r\n          {Array(rating)\r\n            .fill()\r\n            .map((_, i) => (\r\n              <p>‚≠ê</p>\r\n            ))}\r\n        </div>\r\n      </div>\r\n\r\n      <img onMouseDown={revealDetails} src={imgSource} alt='' />\r\n\r\n      <button className=' btn btn-border-3' onClick={addToBasket}>\r\n        Add to Basket\r\n      </button>\r\n\r\n      <div\r\n        onMouseUp={hideDetails}\r\n        id='item__desc__details'\r\n        className={\r\n          isActive ? \"product__display display__active\" : \"product__display\"\r\n        }\r\n      >\r\n        {desc}\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Product;\r\n","C:\\Users\\murry\\Desktop\\REACT\\Ecommerce 1\\electronics\\src\\components\\Checkout.jsx",["94"],"import React from \"react\";\r\nimport \"./styles/Checkout.scss\";\r\nimport Subtotal from \"./Subtotal\";\r\nimport { useStateValue } from \"./StateProvider\";\r\nimport CheckoutProduct from \"./CheckoutProduct\";\r\nimport { motion } from \"framer-motion\";\r\n\r\n\r\nconst Checkout = () => {\r\n  const [{ basket, user },dispatch] = useStateValue();\r\n  \r\n  return (\r\n    <motion.div exit={{opacity:0}} animate={{opacity:1}} transition={{duration:.3}} initial={{opacity:0}} className='checkout'>\r\n      <div className='checkout__top'>\r\n        <div className='checkout__left'>\r\n          <img\r\n            className='checkout__ad'\r\n            src='https://static.vecteezy.com/system/resources/previews/000/286/760/non_2x/vector-merry-christmas-sale-banner-template.jpg'\r\n            alt=''\r\n          />\r\n\r\n          <div>\r\n             <h5>Hi {user?.email  ? user?.email : \"Guest\"}</h5>\r\n            <h2 className='checkout__title'>Your shopping Basket</h2>\r\n          </div>\r\n        </div>\r\n        <div className='checkout__right'>\r\n          <Subtotal />\r\n        </div>\r\n      </div>\r\n      <div className='checkout__items'>\r\n        {basket.map((e) => {\r\n          return (\r\n            <CheckoutProduct\r\n              id={e.id}\r\n              title={e.title}\r\n              price={e.price}\r\n              rating={e.rating}\r\n              imgSource={e.imgSource}\r\n            />\r\n            \r\n          );\r\n        })}\r\n         \r\n      </div>\r\n    </motion.div>\r\n  );\r\n};\r\n\r\nexport default Checkout;\r\n","C:\\Users\\murry\\Desktop\\REACT\\Ecommerce 1\\electronics\\src\\components\\Subtotal.jsx",["95"],"import React from \"react\";\r\nimport \"./styles/Subtotal.scss\";\r\nimport CurrencyFormat from \"react-currency-format\";\r\nimport { useStateValue } from \"./StateProvider\";\r\nimport { getBasketTotal } from \"./reducer\";\r\nimport { useHistory } from \"react-router-dom\";\r\n\r\nfunction Subtotal() {\r\n  const history = useHistory();\r\n  const [{ basket }, dispatch] = useStateValue();\r\n\r\n  return (\r\n    <div className='subtotal'>\r\n      <CurrencyFormat\r\n        renderText={(value) => (\r\n          <>\r\n            <p>\r\n              {/* Part of the homework */}\r\n              Subtotal ({basket.length} items): <strong>{value}</strong>\r\n            </p>\r\n            <small className='subtotal__gift'>\r\n              <input type='checkbox' /> This order contains a gift\r\n            </small>\r\n          </>\r\n        )}\r\n        decimalScale={2}\r\n        value={getBasketTotal(basket)} // Part of the homework\r\n        displayType={\"text\"}\r\n        thousandSeparator={true}\r\n        prefix={\"$\"}\r\n      />\r\n\r\n      <button onClick={(e) => history.push(\"/payment\")}>\r\n        Proceed to Checkout\r\n      </button>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Subtotal;\r\n","C:\\Users\\murry\\Desktop\\REACT\\Ecommerce 1\\electronics\\src\\components\\Shopping.jsx",[],"C:\\Users\\murry\\Desktop\\REACT\\Ecommerce 1\\electronics\\src\\components\\StateProvider.js",[],"C:\\Users\\murry\\Desktop\\REACT\\Ecommerce 1\\electronics\\src\\components\\reducer.js",[],"C:\\Users\\murry\\Desktop\\REACT\\Ecommerce 1\\electronics\\src\\components\\CheckoutProduct.jsx",["96"],"import React from \"react\";\r\nimport \"./styles/CheckoutProduct.scss\";\r\nimport {useStateValue} from \"./StateProvider\";\r\n\r\n\r\nconst CheckoutProduct = ({ id,title, price, imgSource, rating }) => {\r\n  const [{basket},dispatch] = useStateValue();\r\n\r\n  const removeFromBasket = () =>{\r\n    dispatch({\r\n      type: \"REMOVE_FROM_BASKET\",\r\n      id: id,\r\n      \r\n    });\r\n  }\r\n\r\n\r\n\r\n  return (\r\n    <div className='checkout__products'>\r\n      <img src={imgSource} alt='' />\r\n      <div className='desc'>\r\n        <div className='checkproduct__info'>\r\n          <p>{title}</p>\r\n          <p className='checkproduct__price'>\r\n            <small>$</small>\r\n            <strong>{price}</strong>\r\n          </p>\r\n          <div className='checkproduct__rating'>\r\n            {Array(rating)\r\n              .fill()\r\n              .map((_, i) => (\r\n                <p>‚≠ê</p>\r\n              ))}\r\n          </div>\r\n        </div>\r\n\r\n        <button onClick={removeFromBasket}>Remove from Basket</button>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default CheckoutProduct;\r\n","C:\\Users\\murry\\Desktop\\REACT\\Ecommerce 1\\electronics\\src\\components\\Login.jsx",["97"],"import React, {useState} from \"react\";\r\nimport { NavLink, useHistory } from \"react-router-dom\";\r\nimport GraphicEqIcon from \"@material-ui/icons/GraphicEq\";\r\nimport \"./styles/Login.scss\";\r\nimport {auth, db} from \"./Firebase\";\r\nimport { motion } from \"framer-motion\";\r\n\r\nconst Login = () => {\r\n\r\n    const history = useHistory();\r\n    const [email, setEmail] = useState(\"\");\r\n    const [password, setPassword] = useState(\"\");\r\n\r\n    const signIn = e => {\r\n      e.preventDefault();\r\n\r\n      auth\r\n          .signInWithEmailAndPassword(email, password)\r\n          .then((auth) => {\r\n              history.push('/')\r\n             \r\n          })\r\n          .catch(error => alert(error.message))\r\n  }\r\n\r\n    \r\n  const register = e => {\r\n    e.preventDefault();\r\n\r\n    auth\r\n        .createUserWithEmailAndPassword(email, password)\r\n        .then((auth) => {\r\n            // it successfully created a new user with email and password\r\n            if (auth) {\r\n                history.push('/')\r\n              \r\n            }\r\n        })\r\n        .catch(error => alert(error.message))\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n  return (\r\n    <motion.div className='login' initial={{opacity:0}} exit = {{opacity: 0 }}  transition={{duration:.3}} animate = {{opacity:1}}>\r\n         <NavLink className='logo__container2' to='/'>\r\n          <GraphicEqIcon className='header__logo' />\r\n          <h2>Marketarc</h2>\r\n        </NavLink>\r\n      <div className='login__wrapper'>\r\n       \r\n\r\n        <div className='login__formContainer'>\r\n           \r\n          <form className=\"login__form\" action=''>\r\n            <h5>Email</h5>\r\n            <input type='email' value={email} onChange={e => setEmail(e.target.value)} />\r\n            <h5>Password</h5>\r\n            <input type='password' value={password} onChange={e => setPassword(e.target.value)} />\r\n            <button onClick={signIn} className=\"form__login_button btn btn-border-3\">Sign In</button>\r\n          </form>\r\n          <div className=\"login__part2\">\r\n          <p className=\"formp\">\r\n            By signing-in you agree <br/> to Marketarc's Conditions of Use & Sale. <br/>\r\n            Please see our Privacy Notice, our Cookies Notice and our\r\n            Interest-Based Ads Notice\r\n          </p>\r\n\r\n          <button onClick={register} className=\"form__register_button btn btn-border-4\">Not registered yet? Create an Account</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </motion.div>\r\n  );\r\n};\r\n\r\nexport default Login;\r\n","C:\\Users\\murry\\Desktop\\REACT\\Ecommerce 1\\electronics\\src\\components\\Firebase.js",[],"C:\\Users\\murry\\Desktop\\REACT\\Ecommerce 1\\electronics\\src\\components\\Payment.jsx",["98"],"import React, { useState, useEffect } from \"react\";\r\nimport \"./styles/Payment.scss\";\r\nimport { useStateValue } from \"./StateProvider\";\r\nimport CheckoutProduct from \"./CheckoutProduct\";\r\nimport { Link, useHistory } from \"react-router-dom\";\r\nimport { CardElement, useStripe, useElements } from \"@stripe/react-stripe-js\";\r\nimport CurrencyFormat from \"react-currency-format\";\r\nimport { getBasketTotal } from \"./reducer\";\r\nimport axios from \"./Axios\";\r\nimport { db } from \"./Firebase\";\r\nimport { motion } from \"framer-motion\";\r\n\r\nfunction Payment() {\r\n  const [{ basket, user }, dispatch] = useStateValue();\r\n  const history = useHistory();\r\n\r\n  const stripe = useStripe();\r\n  const elements = useElements();\r\n\r\n  const [succeeded, setSucceeded] = useState(false);\r\n  const [processing, setProcessing] = useState(\"\");\r\n  const [error, setError] = useState(null);\r\n  const [disabled, setDisabled] = useState(true);\r\n  const [clientSecret, setClientSecret] = useState(true);\r\n\r\n  useEffect(() => {\r\n    // generate the special stripe secret which allows us to charge a customer\r\n    const getClientSecret = async () => {\r\n      const response = await axios({\r\n        method: \"post\",\r\n        // Stripe expects the total in a currencies subunits\r\n        url: `/payments/create?total=${getBasketTotal(basket) * 100}`,\r\n      });\r\n      setClientSecret(response.data.clientSecret);\r\n    };\r\n\r\n    getClientSecret();\r\n  }, [basket]);\r\n\r\n  console.log(\"THE SECRET IS >>>\", clientSecret);\r\n  console.log(\"üë±\", user);\r\n\r\n  const handleSubmit = async (event) => {\r\n    // do all the fancy stripe stuff...\r\n    event.preventDefault();\r\n    setProcessing(true);\r\n\r\n    const payload = await stripe\r\n      .confirmCardPayment(clientSecret, {\r\n        payment_method: {\r\n          card: elements.getElement(CardElement),\r\n        },\r\n      })\r\n      .then(({ paymentIntent }) => {\r\n        // paymentIntent = payment confirmation\r\n\r\n        db.collection(\"users\")\r\n          .doc(user?.uid)\r\n          .collection(\"orders\")\r\n          .doc(paymentIntent.id)\r\n          .set({\r\n            basket: basket,\r\n            amount: paymentIntent.amount,\r\n            created: paymentIntent.created,\r\n          });\r\n\r\n        setSucceeded(true);\r\n        setError(null);\r\n        setProcessing(false);\r\n\r\n        dispatch({\r\n          type: \"EMPTY_BASKET\",\r\n        });\r\n\r\n        history.replace(\"/orders\");\r\n      });\r\n  };\r\n\r\n  const handleChange = (event) => {\r\n    // Listen for changes in the CardElement\r\n    // and display any errors as the customer types their card details\r\n    setDisabled(event.empty);\r\n    setError(event.error ? event.error.message : \"\");\r\n  };\r\n\r\n  return (\r\n    <motion.div exit={{opacity:0}} animate={{opacity:1,transition:\"all 1s ease-in-out\"}} initial={{opacity:0}} className='payment'>\r\n      <div className='payment__container'>\r\n        <h1>\r\n          Checkout (<Link to='/checkout'>{basket?.length} items</Link>)\r\n        </h1>\r\n\r\n        {/* Payment section - delivery address */}\r\n        <div className='payment__section'>\r\n          <div className='payment__title'>\r\n            <h3>Delivery Address</h3>\r\n          </div>\r\n          <div className='payment__address'>\r\n            <p>{user?.email}</p>\r\n            <p>123 React Lane</p>\r\n            <p>Los Angeles, CA</p>\r\n          </div>\r\n        </div>\r\n\r\n        {/* Payment section - Review Items */}\r\n        <div className='payment__section'>\r\n          <div className='payment__title'>\r\n            <h3>Review items and delivery</h3>\r\n          </div>\r\n          <div className='payment__items'>\r\n            {basket.map((item) => (\r\n              <CheckoutProduct\r\n                id={item.id}\r\n                title={item.title}\r\n                image={item.image}\r\n                price={item.price}\r\n                rating={item.rating}\r\n              />\r\n            ))}\r\n          </div>\r\n        </div>\r\n\r\n        {/* Payment section - Payment method */}\r\n        <div className='payment__section'>\r\n          <div className='payment__title'>\r\n            <h3>Payment Method</h3>\r\n          </div>\r\n          <div className='payment__details'>\r\n            {/* Stripe magic will go */}\r\n\r\n            <form onSubmit={handleSubmit}>\r\n              <CardElement onChange={handleChange} />\r\n\r\n              <div className='payment__priceContainer'>\r\n                <CurrencyFormat\r\n                  renderText={(value) => <h3>Order Total: {value}</h3>}\r\n                  decimalScale={2}\r\n                  value={getBasketTotal(basket)}\r\n                  displayType={\"text\"}\r\n                  thousandSeparator={true}\r\n                  prefix={\"$\"}\r\n                />\r\n                <button disabled={processing || disabled || succeeded}>\r\n                  <span>{processing ? <p>Processing</p> : \"Buy Now\"}</span>\r\n                </button>\r\n              </div>\r\n\r\n              {/* Errors */}\r\n              {error && <div>{error}</div>}\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </motion.div>\r\n  );\r\n}\r\n\r\nexport default Payment;\r\n","C:\\Users\\murry\\Desktop\\REACT\\Ecommerce 1\\electronics\\src\\components\\Axios.js",[],"C:\\Users\\murry\\Desktop\\REACT\\Ecommerce 1\\electronics\\src\\components\\Order.js",[],"C:\\Users\\murry\\Desktop\\REACT\\Ecommerce 1\\electronics\\src\\components\\Footers.jsx",["99","100","101","102"],"import React, { Component } from \"react\";\r\nimport \"./styles/Footer.scss\";\r\nimport { motion } from \"framer-motion\";\r\nimport { Link } from \"react-router-dom\";\